pipeline {
	agent any
	stages {
		stage('build'){
			steps {
				echo "build script"
				echo "this stage version is build"
				}
			}
		stage('test'){
			steps {
				echo "test script"
				echo "this environment is test "
				}
			}
		stage('deploy'){
			steps {
				echo "test script"
				echo "this environment is deploy"
				}	
			
		}
	}
	
}
